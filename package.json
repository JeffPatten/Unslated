{
  "name": "Unslated",
  "host": [],
  "port": "8080",
  "version": "0.0.1",
  "description": "Boilerplate for component-based UI development",
  "repository": "connectivedx/unslated",
  "bugs": {
    "url": "https://github.com/connectivedx/unslated/issues"
  },
  "author": "Connective DX",
  "license": "MIT",
  "directories": {
    "source": "./src",
    "dest": "./dist",
    "paths": "/",
    "assets": "assets"
  },
  "scripts": {
    "dev": "webpack-dev-server --config build/configs/webpack.config.js --mode development --open",
    "build": "webpack --config build/configs/webpack.config.js --mode production",
    "watch": "webpack --config build/configs/webpack.confg.js --watch && webpack-dev-server",
    "new:page":     "node build/scaffolding/scaffolding.build.js pages",
    "new:atom":     "node build/scaffolding/scaffolding.build.js atoms",
    "new:molecule": "node build/scaffolding/scaffolding.build.js molecules",
    "new:organism": "node build/scaffolding/scaffolding.build.js organisms",
    "new:modifier": "node build/scaffolding/scaffolding.build.js modifiers",
    "new:template": "node build/scaffolding/scaffolding.build.js templates",
    "new:variable": "node build/scaffolding/scaffolding.build.js variables"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chart.js": "^2.7.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.0.4",
    "docgen-loader": "^1.3.4",
    "enhanced-resolve": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.5",
    "postcss": "^7.0.7",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^3.0.0",
    "pretty": "^2.0.0",
    "prismjs": "^1.15.0",
    "react": "^16.7.0",
    "react-docgen": "^2.21.0",
    "react-dom": "^16.7.0",
    "react-element-to-string": "^1.0.2",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "reset-css": "^4.0.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.11",
    "webpack-monitor": "^1.0.14",
    "webpack-node-externals": "^1.7.2",
    "webpack-svgstore-plugin": "github:drolsen/webpack-svgstore-plugin"
  },
  "dependencies": {}
}
